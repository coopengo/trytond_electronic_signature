<data>
    <xpath expr="/form/notebook" position="inside">
        <page id="e-signature" string="Electronic Signature">
            <field name="signatures" colspan="4" mode="form,tree"/>
            <button string="Bypass Digital Process" name="manually_create_forced_signature" colspan="2"/>
